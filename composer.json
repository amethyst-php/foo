{
    "name": "amethyst/foo",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-foo", "foo"],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "amethyst/core": "0.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "foo",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\FooServiceProvider"
            ]
        }
    }
}
